<h1><%= @cocktail.name %></h1>
<% @cocktail.doses.each do |dose|%>
<ul>
  <li>
    <%= dose.description %> -
    <%= dose.ingredient.name %> <%= link_to "Remove", dose_path(dose), method: :delete, data: {confirm: "Confirm?"} %>
  </li>
</ul>
<% end %>
<%= render "/doses/form", cocktail: @cocktail, dose: @dose %>

